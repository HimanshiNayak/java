class Person {
  String name;
  int age;
  void display(){
    System.out.print("Name : "+name+"  , age : "+age);
   }
}
class Student extends Person{
  int rollno;
  //if both have a func with same name then will run only the func of the  class whose obj is amde: scope resolution
  void dis(){
    System.out.println(" roll no : "+rollno);
   }
}

public class inheritance {
public static void main(String[] args) {
    Student s = new Student();
    s.name = "Ravi";
    s.age = 35;
    s.rollno = 45;
    // s.display();
   s.display();
    s.dis();
    }
}

//no multiple inheritance is there in java due to ambiguity

//multilvel inheritance works
public class multilevel {
    public static void main(String[] args) {

        C c = new C();
        c.printmesaage();
        c.show();
        c.display();

        B b = new B();
        b.display();
        b.show();
// b.printmesaage :cannotbe accesssed as is the method od child class

        A a = new A();
        a.display();
        //here the methods of b and c cannot be accessed
    }
}
class A{
 void display(){
System.out.println("bye, class A");
}
}
class B extends A{
     void show(){
        System.out.println("hi , Class B");
        }
}
class C extends B{
    void printmesaage(){
        System.out.println("hello, class c");
        }
}
